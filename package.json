{
  "name": "@caviajs/http",
  "description": "ecosystem for your guinea pig",
  "version": "0.1.6",
  "author": "Pawe≈Ç Partyka",
  "license": "MIT",
  "homepage": "https://caviajs.com",
  "repository": "caviajs/http",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "generate-contract": "dist/contract/generate-contract.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register jest --runInBand",
    "test:watch": "jest --watch",
    "release": "npm run build && npm publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "colorette": "~2.0.19",
    "iconv-lite": "~0.6.3",
    "no-case": "~3.0.4",
    "path-to-regexp": "~6.2.1",
    "prettier": "~2.7.1",
    "qs": "~6.11.0",
    "yargs": "~17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~17.1.2",
    "@commitlint/config-conventional": "~17.1.0",
    "@release-it/conventional-changelog": "~4.3.0",
    "@types/jest": "~27.4.1",
    "@types/node": "~18.7.23",
    "@types/qs": "~6.9.6",
    "@types/supertest": "~2.0.12",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "~5.38.1",
    "@typescript-eslint/parser": "~5.38.1",
    "eslint": "~8.24.0",
    "husky": "~8.0.1",
    "jest": "~27.5.1",
    "release-it": "~14.12.1",
    "rimraf": "~3.0.2",
    "rxjs": "~7.5.7",
    "supertest": "~6.2.4",
    "ts-jest": "~27.1.4",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "rxjs": "~7.5.6"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "test"
    ],
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
